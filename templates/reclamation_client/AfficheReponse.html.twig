{% extends 'base.html.twig' %}



{%  block body   %}




    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <h1>les Réponse de Administrateur
                </h1>
                <table id="example" class="table table-striped table-bordered" style="width:100%">

                    <thead>
                    <tr>
                        <th>ID</th>
                        <th>Message</th>
                        <th>Date de Réponse </th>

                    </tr>
                    </thead>
                    <tbody>

                    <tr  {% for a in reclamationAdmins %}>

                        <td>{{ a.id }}</td>

                        <td>{{ a.message }}</td>
                        <td> {{ a.date|date('d/m/Y') }} </td>



                    </tr {% endfor %}  >



                    </tbody>

                </table>
            </div></div></div>
    <script>$(document).ready(function() {
            $('#example').DataTable();
        } );</script>




{% endblock %}

